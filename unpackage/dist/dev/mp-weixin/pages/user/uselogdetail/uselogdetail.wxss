.animate {
  position: relative;
  width: 380rpx;
  height: 380rpx;
  overflow: hidden;
  margin: auto;
}
.circle {
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -130rpx;
  margin-top: -130rpx;
  height: 260rpx;
  width: 260rpx;
}
.point {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.locking .point {
  -webkit-animation: animations 1s forwards infinite;
          animation: animations 1s forwards infinite;
}
.closeing .point {
  -webkit-animation: animations 1s forwards infinite;
          animation: animations 1s forwards infinite;
}
.locking .circle {
  -webkit-animation: rotate 1.8s linear infinite;
          animation: rotate 1.8s linear infinite;
}
.locking .center {
  background-color: #38A0FE;
  box-shadow: none;
  color: #fff;
}
.locking .center {
  background-color: #38A0FE;
  box-shadow: none;
  color: #fff;
}
.locked .center {
  box-shadow: 0px 0px 8px 12px #f8ecec;
}
.closeing .circle {
  -webkit-animation: closing 1.8s linear infinite;
          animation: closing 1.8s linear infinite;
}
.closeing .center {
  background-color: #fa6b6b;
  box-shadow: none;
  color: #fff;
}
.center {
  position: absolute;
  width: 165rpx;
  height: 165rpx;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  border-radius: 50%;
  background-color: #fff;
  box-shadow: 0px 0px 8px 12px #e9f1f8;
  text-align: center;
  font-size: 14px;
  color: #000;
}
.icons {
  margin-top: 12px;
  width: 25px;
  height: 30px;
}
@-webkit-keyframes animations {
0% {
    -webkit-transform: scale(0.8);
            transform: scale(0.8);
}
100% {
    -webkit-transform: scale(0.9);
            transform: scale(0.9);
}
}
@keyframes animations {
0% {
    -webkit-transform: scale(0.8);
            transform: scale(0.8);
}
100% {
    -webkit-transform: scale(0.9);
            transform: scale(0.9);
}
}
@-webkit-keyframes rotate {
0% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
@keyframes rotate {
0% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
@-webkit-keyframes closing {
0% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
100% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
}
@keyframes closing {
0% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
100% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
}
.uselist {
  background-color: #ffffff;
  padding: 15px;
  border-radius: 4px;
  margin-bottom: 15px;
}
.usenames {
  display: flex;
  align-items: flex-start;
  justify-content: space-between;
}
.status {
  display: flex;
  align-items: center;
  font-size: 14px;
}
.has {
  color: #45C08C;
}
.playing {
  color: #38A0FE;
}
.finish {
  color: #FEB749;
}
.mastname {
  display: flex;
  align-items: center;
  margin-bottom: 15px;
}
.mastname > text:nth-child(1) {
  margin-right: 5px;
  font-size: 16px;
  font-weight: bold;
  color: #333333;
}
.mastname > text:nth-child(2) {
  font-size: 14px;
  color: #999;
}
.other {
  display: flex;
  align-items: center;
  color: #999;
  font-size: 14px;
  margin-bottom: 8px;
}
.btnlist {
  display: flex;
  align-items: center;
  justify-content: flex-end;
  margin-top: 10px;
  padding-top: 10px;
  font-size: 14px;
  border-top: 1px solid #DEDEDE;
}
.cancel {
  width: 80px;
  line-height: 30px;
  color: #45C08C;
  border: 1px solid #45C08C;
  text-align: center;
  margin-right: 10px;
  border-radius: 2px;
}
.over {
  width: 80px;
  line-height: 30px;
  color: #fff;
  background: #FB6A6A;
  text-align: center;
  margin-right: 10px;
  border-radius: 2px;
}
.notime {
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 1000;
  top: 0;
  left: 0;
}

